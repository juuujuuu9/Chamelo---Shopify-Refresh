{% schema %}
  {
    "name": "Index Object Instagram",
    "class": "Index_object_instagram_new",
    "settings": [
      {
        "type": "color",
        "id": "main_background_color",
        "label": "Add Main Background Color"
      },
      {
        "type": "image_picker",
        "id": "main_custom_image",
        "label": "Add Main Custom Image"
      },
      {
        "type": "textarea",
        "id": "main_heading_text",
        "label": "Add Main Heading Text",
        "default": "Say hello to Chamelo"
      },
      {
        "type": "text",
        "id": "main_button_text",
        "label": "Add Main Button Text",
        "default": "Follow @Hellochamelo"
      },
      {
        "type": "url",
        "id": "main_button_link",
        "label": "Add Main Button Link",
        "default": "/"
      }
    ],
    "presets": [
      {
        "name": "Index Object Instagram"
      }
    ]
  }
{% endschema %}