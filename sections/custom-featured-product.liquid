    </script>
    <script src="{{ 'product-model.js' | asset_url }}" defer></script>
  {%- endif -%}

  {%- liquid
    if product.selected_or_first_available_variant.featured_media
      assign seo_media = product.selected_or_first_available_variant.featured_media
    else
      assign seo_media = product.featured_media
    endif
  -%}

  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>

  {% if product.media.size > 0 %}
    <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
  {% endif %}
</product-info>

<script>
    // Handle Frame Color Selection
document.querySelectorAll('[name="frame_color"]').forEach(radio => {
  radio.addEventListener('change', (e) => {
    const frameColorProperty = document.querySelector('[js-hb-frame-color-property]');
    const selectedLabel = document.querySelector('[js-selected-frame-color]');
    
    if (e.target.checked) {
      const colorName = e.target.dataset.colorName;
      
      if (frameColorProperty) {
        frameColorProperty.value = colorName;
      }
      
      if (selectedLabel) {
        selectedLabel.textContent = colorName;
      }
    }
  });
});

// Handle Lens Color Selection
document.querySelectorAll('[name="lens_color"]').forEach(radio => {
  radio.addEventListener('change', (e) => {
    const lensColorProperty = document.querySelector('[js-hb-lens-color-property]');
    const selectedLabel = document.querySelector('[js-selected-lens-color]');
    
    if (e.target.checked) {
      const colorName = e.target.dataset.colorName;
      
      if (lensColorProperty) {
        lensColorProperty.value = colorName;
      }
      
      if (selectedLabel) {
        selectedLabel.textContent = colorName;
      }
    }
  });
});

// Handle Audio Selection
document.querySelectorAll('[name="audio"]').forEach(radio => {
  radio.addEventListener('change', (e) => {
    const audioProperty = document.querySelector('[js-hb-audio-property]');
    const selectedLabel = document.querySelector('[js-selected-audio]');
    
    if (e.target.checked) {
      const audioValue = e.target.value;
      
      if (audioProperty) {
        audioProperty.value = audioValue;
      }
      
      if (selectedLabel) {
        selectedLabel.textContent = audioValue;
      }
    }
  });
});

// Handle Upsell Products
document.querySelectorAll('[js-hb-upsell-product-radio-btns]').forEach(radio => {
  radio.addEventListener('change', () => {
    const checked = radio.checked;
    const parent = radio.closest('[js-hb-upsell-products]');
    
    if(checked) {
      const formId = radio.getAttribute('data-form-ids');
      parent.querySelector('[js-hb-upsell-product-id-inputs]').setAttribute('form', formId);
    } else {
      parent.querySelector('[js-hb-upsell-product-id-inputs]').setAttribute('form', '');
    }
    
    updateMainProductUpsells();
  });
});

function updateMainProductUpsells() {
  const mainPropertyInput = document.querySelector('[js-hb-main-product-upsells]');
  
  if (!mainPropertyInput) return;
  
  const selectedUpsells = [];
  
  document.querySelectorAll('[js-hb-upsell-product-radio-btns]:checked').forEach(radio => {
    const parent = radio.closest('[js-hb-upsell-products]');
    const productNameInput = parent.querySelector('[js-hb-upsell-product-names]');
    
    if (productNameInput) {
      const productName = productNameInput.getAttribute('data-product-names');
      selectedUpsells.push(productName);
    }
  });
  
  mainPropertyInput.value = selectedUpsells.length > 0 ? selectedUpsells.join(', ') : '';
}

// Initialize on page load
document.addEventListener('DOMContentLoaded', function() {
  // Set initial values
  const selectedFrameColor = document.querySelector('[name="frame_color"]:checked');
  if (selectedFrameColor) {
    const frameColorProperty = document.querySelector('[js-hb-frame-color-property]');
    if (frameColorProperty) {
      frameColorProperty.value = selectedFrameColor.dataset.colorName;
    }
  }
  
  const selectedLensColor = document.querySelector('[name="lens_color"]:checked');
  if (selectedLensColor) {
    const lensColorProperty = document.querySelector('[js-hb-lens-color-property]');
    if (lensColorProperty) {
      lensColorProperty.value = selectedLensColor.dataset.colorName;
    }
  }
  
  const selectedAudio = document.querySelector('[name="audio"]:checked');
  if (selectedAudio) {
    const audioProperty = document.querySelector('[js-hb-audio-property]');
    if (audioProperty) {
      audioProperty.value = selectedAudio.value;
    }
  }
  
  updateMainProductUpsells();
});
</script>
