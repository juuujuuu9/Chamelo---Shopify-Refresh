      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    }
  ]
}
{% endschema %}

{% style %}
  {%- for block in section.blocks -%}
    {%- if block.type == 'custom-text-with-style' -%}
      .custom-text-block-{{ block.id }} {
        /* Typography */
        font-size: {{ block.settings.font_size | default: 16 }}px;
        line-height: {{ block.settings.line_height | default: 1.4 }};
        font-weight: {{ block.settings.font_weight | default: 400 }};
        text-align: {{ block.settings.text_align | default: 'left' }};
        text-transform: {{ block.settings.text_transform | default: 'none' }};
        
        /* Colors */
        color: {{ block.settings.text_color | default: '#000000' }};
        background-color: {{ block.settings.background_color | default: 'transparent' }};
        
        /* Spacing */
        margin-top: {{ block.settings.margin_top | default: 0 }}px;
        margin-bottom: {{ block.settings.margin_bottom | default: 0 }}px;
        padding-top: {{ block.settings.padding_top | default: 0 }}px;
        padding-bottom: {{ block.settings.padding_bottom | default: 0 }}px;
        padding-left: {{ block.settings.padding_left | default: 0 }}px;
        padding-right: {{ block.settings.padding_right | default: 0 }}px;
        
        /* Border & Effects */
        border-radius: {{ block.settings.border_radius | default: 0 }}px;
        border-width: {{ block.settings.border_width | default: 0 }}px;
        border-color: {{ block.settings.border_color | default: '#000000' }};
        border-style: {{ block.settings.border_style | default: 'none' }};
        
        /* Box Shadow */
        {%- if block.settings.box_shadow_blur > 0 -%}
          box-shadow: 0 0 {{ block.settings.box_shadow_blur }}px {{ block.settings.box_shadow_spread }}px {{ block.settings.box_shadow_color }};
        {%- endif -%}
        
        /* Text Effects */
        {%- if block.settings.text_underline -%}
          text-decoration: underline;
        {%- endif -%}
        
        /* Rich Text Strike Out Styling */
        {%- if block.settings.enable_strike_out_rich_text -%}
          s, strike, del, .strikethrough {
            text-decoration: line-through !important;
            color: {{ block.settings.strike_out_color | default: '#999999' }} !important;
            opacity: {{ block.settings.strike_out_opacity | default: 0.7 }} !important;
          }
        {%- endif -%}
        
        {%- if block.settings.text_italic -%}
          font-style: italic;
        {%- endif -%}
        
        /* Text Shadow */
        {%- if block.settings.text_shadow_blur > 0 -%}
          text-shadow: 0 0 {{ block.settings.text_shadow_blur }}px {{ block.settings.text_shadow_color }};
        {%- endif -%}
      }
      
      /* Mobile Responsive Styles */
      @media screen and (max-width: 768px) {
        .custom-text-block-{{ block.id }} {
          font-size: {{ block.settings.mobile_font_size | default: 14 }}px;
          margin-top: {{ block.settings.mobile_margin_top | default: 0 }}px;
          margin-bottom: {{ block.settings.mobile_margin_bottom | default: 0 }}px;
        }
      }
    {%- endif -%}
  {%- endfor -%}
{% endstyle %}
