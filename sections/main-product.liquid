<script>
  (() => {
    const shippingDateTag = document.querySelector('[js-hb-shipping-date]');
    const shippingDate = document.querySelector('[data-hb-shipping-date]')?.getAttribute('data-hb-shipping-date')
    if(shippingDateTag) {
      shippingDateTag.innerText = getShippingDay(Number(shippingDate));
    }
    function getShippingDay(days) {
      const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
      const currentDate = new Date();
      currentDate.setDate(currentDate.getDate() + days);;
  
      const month = monthNames[currentDate.getMonth()];
      const day = currentDate.getDate();
      return `${month} ${day}`;
    }
  })()
</script>

