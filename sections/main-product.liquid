<product-info
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }} Product_template_main_new {% unless product.metafields.custom.product_mobile_version_2 == true %}Product_version_1_new {% endunless %}"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="true"
  data-url="{{ product.url }}"
  {% if section.settings.image_zoom == 'hover' %}
    data-zoom-on-hover
  {% endif %}
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
  {% if template.suffix == 'accessories-new'%}
    {{ 'accessories-product-template.css' | asset_url | stylesheet_tag }}
  {% endif %}

  {% unless product.has_only_default_variant %}
    {{ 'component-product-variant-picker.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch-input.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch.css' | asset_url | stylesheet_tag }}
  {% endunless %}
  {%- if product.quantity_price_breaks_configured? -%}
    {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
  {%- endif -%}

  {%- style -%}

     {% if localization.country.iso_code != 'US' %}
      trynow-cta-button{
        display: none !important;
      }
    {% endif %}
    
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
    
    .product__title-price-flex {
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      align-items: center;
    }
    .product__title-price-adjacent{
      display: flex;
      justify-content: flex-start;
      flex-direction: row;
      gap: 10px;
      align-items: center;
    }

    .product__title-price-adjacent .Product_template_rightside_mainboths_mainprice_new{
      padding-top:0;
    }

    {%if template.suffix == 'aura-audio'%}
        .Meta_stitch_product_list_main_new{
        /*  display: none; */
        }
    {% endif %}
   
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {%- if product.quantity_price_breaks_configured? -%}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
  <div id="product_top"></div>
  <div class="page-width">
    <div id="product-container" class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %} Product_template_inner_new">
      <div class="grid__item product__media-wrapper Product_template_leftside_new">
        {% render 'product-media-gallery', variant_images: variant_images %}
        {% if template.suffix != 'accessories-new'%}
          {%- if section.blocks.size > 0 -%}
            <div class="Product_template_leftside_mainloops_new Desktop_only_new">
              <div class="Product_template_leftside_innerloops_new">
                {%- for block in section.blocks -%}
                  {%- case block.type -%}
                    {%- when 'meta_accordions_list' -%}
                      {%- if product.metafields.custom.product_accordions_details_text != blank or product.metafields.custom.product_accordions_what_s_in_the_box_text != blank or product.metafields.custom.product_accordions_size_and_fit_text != blank or block.settings.main_information_text_4 != blank -%}
                        <div class="Product_template_leftside_innerloops_mainboxs_new" {{ block.shopify_attributes }}>
                          <div class="Product_template_leftside_innerloops_innerboxs_new">
                            {%- render 'Product-meta-accordions-list-new', block: block -%}
                          </div>
                        </div>
                      {%- endif -%} 
                  {%- endcase -%}
                {%- endfor -%}  
              </div>
            </div>
          {%- endif -%}
        {%- endif -%}
      </div>
      <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %} Product_template_rightside_new">
      
        <section
          id="ProductInfo-{{ section.id }}"
          class="product__info-container {% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
        >
          {%- assign product_form_id = 'product-form-' | append: section.id -%}

          <div class="tryme__mobile">
            {% comment %} {% if product.selected_or_first_available_variant.metafields.custom.tryme_code != blank %}
              {{ product.selected_or_first_available_variant.metafields.custom.tryme_code }}
            {% endif %} {% endcomment %}
            {% if product.selected_or_first_available_variant.metafields.custom.tryme_code != blank %}
              <div class="glasses-wrap">
                <button data-vto-model-trigger class="varai-btn-base varai-btn-custom">
                  <span>{% render 'icons' with 'vto' %}</span>
                    <span>VIRTUAL TRY-ON</span>
                </button>
              </div>
            {% endif %}
            {% if product.selected_or_first_available_variant.metafields.custom.tryme_3d_viewer_code != blank %}
              <div class="tryme__3d-view">
                <button data-3d-model-trigger class="varai-btn-base varai-btn-default-style"> 
                    <span>{% render 'icons' with '3d-view' %}</span>
                    <span>3D PRODUCT VIEW</span>
                  </button>
              </div>
            {% endif %}
          </div> 

          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                {% render block %}
              {%- when 'custom_review' -%}
                <div class="jdgm_widget_new Productjdgm_widget_new">
                  <a href="#Reviews" class="jdgm_widget_link_new"></a>
                  <!-- Start of Judge.me code -->
                    <div class='jdgm-widget jdgm-preview-badge'>
                      {{ product.metafields.judgeme.badge }}
                    </div>
                  <!-- End of Judge.me code -->
                </div>
              {%- when 'custom_count_tag_text' -%}
                <div
                  class="Custom_count_tag_text_main_new product__inventory{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}{% if product.selected_or_first_available_variant.inventory_management != 'shopify' %} visibility-hidden{% endif %}"
                  {{ block.shopify_attributes }}
                  id="Inventory-{{ section.id }}"
                  role="status"
                >
                  {%- if product.selected_or_first_available_variant.inventory_management == 'shopify' -%}
                    {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                      {%- if product.selected_or_first_available_variant.inventory_quantity
                          <= block.settings.inventory_threshold_custom
                      -%}
                        {%- if product.metafields.custom.product_tags_background_video_or_image.value != blank -%}
                          <div class="Custom_count_tag_text_static_new">
                            {%- if product.metafields.custom.product_tags_background_video_or_image.value contains "files" -%}
                              <div class="Custom_count_tag_text_static_imagevideo_new">
                                {{
                                  product.metafields.custom.product_tags_background_video_or_image.value | 
                                  image_url: width: img.width | 
                                  image_tag: widths: '750, 900, 1080, 1296, 1512, 1728, 1944, 2048', 
                                  fetchpriority: 'auto', 
                                  class:'Static_image_new'
                                }}
                              </div>
                            {%- else -%}
                              <div class="Custom_count_tag_text_static_imagevideo_new">
                                {{ product.metafields.custom.product_tags_background_video_or_image.value | video_tag: class:'Static_video_new', image_size: 'master', controls: false, autoplay: true, loop: true }}
                              </div>
                            {%- endif -%}
                          </div>
                        {%- endif -%}
                        <div class="Custom_count_tag_text_overlay_new">
                          <svg width="12" height="16" viewBox="0 0 12 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M9.73773 7.40774C9.57712 7.18551 9.3816 6.99292 9.20005 6.80032C8.7322 6.35586 8.2015 6.03733 7.7546 5.57065C6.71416 4.48914 6.48372 2.70391 7.14709 1.3335C6.48372 1.50387 5.90415 1.88907 5.40836 2.3113C3.59981 3.85209 2.88756 6.57068 3.73946 8.90408C3.76739 8.97816 3.79533 9.05223 3.79533 9.14853C3.79533 9.3115 3.69058 9.45965 3.55093 9.51891C3.39032 9.59299 3.22273 9.54854 3.09006 9.43002C3.05043 9.3948 3.01728 9.35211 2.9923 9.30409C2.20324 8.2448 2.07755 6.72624 2.60824 5.51139C1.44211 6.51883 0.806667 8.22258 0.897444 9.83003C0.939341 10.2004 0.981238 10.5708 1.09995 10.9412C1.19771 11.3856 1.38624 11.8301 1.59573 12.2227C2.34988 13.5042 3.65567 14.4228 5.05922 14.6079C6.55355 14.808 8.15262 14.5191 9.29781 13.4227C10.5757 12.1931 11.0226 10.2226 10.3662 8.5337L10.2754 8.3411C10.1288 8.00035 9.73773 7.40774 9.73773 7.40774ZM7.53115 12.0745C7.33563 12.2523 7.01442 12.4449 6.76304 12.519C5.98096 12.8153 5.19888 12.4005 4.73801 11.9116C5.56897 11.7042 6.06475 11.0523 6.21139 10.393C6.3301 9.8004 6.10665 9.3115 6.01587 8.74111C5.93208 8.19295 5.94604 7.72627 6.13458 7.21514C6.26725 7.49663 6.40691 7.77812 6.5745 8.00035C7.11218 8.74111 7.9571 9.06705 8.13866 10.0745C8.16659 10.1782 8.18055 10.2819 8.18055 10.393C8.2015 11.0004 7.95012 11.6671 7.53115 12.0745Z" fill="white"/></svg>
                          {%- if block.settings.show_inventory_quantity -%}
                            {{-
                              'products.product.inventory_low_stock_show_count'
                              | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                            -}}
                          {%- else -%}
                            {{- 'products.product.inventory_low_stock' | t -}}
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                    {%- endif -%}
                  {%- endif -%}
                </div>
                            
              {%- when 'text' -%}
                <p
                  class="product__text inline-richtext{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"
                  {{ block.shopify_attributes }}
                >
                  {{- block.settings.text -}}
                </p>

                {%- when 'text-custom' -%}
                  {% if block.settings.text_custom != blank %}
                  <h6
                    class="!tw-my-[1rem] custom_text_new tw-font-bold {%if block.settings.make_center == true %} tw-text-center {% endif %}"
                    {{ block.shopify_attributes }}
                  >
                    {{- block.settings.text_custom -}}
                  </h6>
                  {% endif %}
                  {%- when 'text-below-button' -%}
                    {% if block.settings.hide_text_text == false %}
                    <h6
                      class="!tw-my-[1rem] custom_text_new tw-font-bold {%if block.settings.make_center == true %} tw-text-center {% endif %}"
                      {{ block.shopify_attributes }}
                    >
                      {{- block.settings.text_custom -}}
                    </h6>
                    {% endif %}
              {%- when 'title' -%}
                <div class="product__title Product_template_rightside_mainboths_new {% if block.settings.price_title_flex == true %} product__title-price-flex{% endif %} {% if block.settings.price_title_adjacent == true %}product__title-price-adjacent{% endif %}" {{ block.shopify_attributes }}>
                  <h1>{{ product.title | escape }}</h1>
                  <a href="{{ product.url }}" class="product__title">
                    <h2 class="h1">
                      {{ product.title | escape }}
                    </h2>
                  </a>

                  {% if template.suffix != 'accessories-new' %}
                    <div class="Product_template_rightside_mainboths_mainprice_new">
                      <div class="Product_template_rightside_mainboths_innerprice_new">
                        <div id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                          {%- render 'price',
                            product: product,
                            use_variant: true,
                            show_badges: true,
                            price_class: 'price--large'
                            display_percentage: true 
                          -%}
                        </div>
                        {%- if product.quantity_price_breaks_configured? -%}
                          <div class="volume-pricing-note" id="Volume-Note-{{ section.id }}">
                            <span>{{ 'products.product.volume_pricing.note' | t }}</span>
                          </div>
                        {%- endif -%}
                        {% comment %}
                        {%- if cart.taxes_included or cart.duties_included or shop.shipping_policy.body != blank -%}
                          <div class="product__tax caption rte">
                            {%- if cart.duties_included and cart.taxes_included -%}
                              {{ 'products.product.duties_and_taxes_included' | t }}
                            {%- elsif cart.taxes_included -%}
                              {{ 'products.product.taxes_included' | t }}
                            {%- elsif cart.duties_included -%}
                              {{ 'products.product.duties_included' | t }}
                            {%- endif -%}
                            {%- if shop.shipping_policy.body != blank -%}
                              {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                            {%- endif -%}
                          </div>
                        {%- endif -%}
                        <div {{ block.shopify_attributes }}>
                          {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                          {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                            <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                            {{ form | payment_terms }}
                          {%- endform -%}
                        </div>
                        {% endcomment %}
                      </div>
                    </div>
                  {% endif %}
                </div>
              {%- when 'price' -%}
                <div id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                  {%- render 'price',
                    product: product,
                    use_variant: true,
                    show_badges: true,
                    price_class: 'price--large'
                  -%}
                </div>
                {%- if product.quantity_price_breaks_configured? -%}
                  <div class="volume-pricing-note" id="Volume-Note-{{ section.id }}">
                    <span>{{ 'products.product.volume_pricing.note' | t }}</span>
                  </div>
                {%- endif -%}
                {%- if cart.taxes_included or cart.duties_included or shop.shipping_policy.body != blank -%}
                  <div class="product__tax caption rte">
                    {%- if cart.duties_included and cart.taxes_included -%}
                      {{ 'products.product.duties_and_taxes_included' | t }}
                    {%- elsif cart.taxes_included -%}
                      {{ 'products.product.taxes_included' | t }}
                    {%- elsif cart.duties_included -%}
                      {{ 'products.product.duties_included' | t }}
                    {%- endif -%}
                    {%- if shop.shipping_policy.body != blank -%}
                      {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                    {%- endif -%}
                  </div>
                {%- endif -%}
                <div {{ block.shopify_attributes }}>
                  {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                  {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                    <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                    {{ form | payment_terms }}
                  {%- endform -%}
                </div>
              {%- when 'meta_short_description' -%}
                {% if template.suffix != 'accessories-new' %}
                  {%- if product.metafields.custom.product_short_description_text != blank -%}
                    <div class="Meta_short_description_main_new" {{ block.shopify_attributes }}>
                      <div class="Meta_short_description_inner_new">
                        {{ product.metafields.custom.product_short_description_text | metafield_tag }}
                      </div>
                    </div>
                  {%- endif -%}  
                {%- endif -%}  
              {%- when 'object_size_and_fit' -%}
                {%- if product.metafields.custom.product_size_fit_popup_item_selections.value != blank -%}
                  {%- if block.settings.main_heading_text != blank -%}
                    <div class="Object_size_and_fit_main_new" {{ block.shopify_attributes }}>
                      <div class="Object_size_and_fit_inner_new open_size_popup">
                        <span class="Object_size_and_fit_infos_new">
                          {{ block.settings.main_heading_text }}
                        </span>
                        <span class="Object_size_and_fit_icons_new">
                          <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><g clip-path="url(#clip0_9002_5432)"><path d="M9.96111 7.38905C10.3907 7.38905 10.7389 7.04083 10.7389 6.61127C10.7389 6.18172 10.3907 5.8335 9.96111 5.8335C9.53156 5.8335 9.18333 6.18172 9.18333 6.61127C9.18333 7.04083 9.53156 7.38905 9.96111 7.38905Z" fill="black"/><path d="M11.6667 12.7779H10.5556V8.3335H8.88889C8.74155 8.3335 8.60024 8.39203 8.49605 8.49621C8.39187 8.6004 8.33333 8.74171 8.33333 8.88905C8.33333 9.03639 8.39187 9.1777 8.49605 9.28189C8.60024 9.38608 8.74155 9.44461 8.88889 9.44461H9.44445V12.7779H8.33333C8.18599 12.7779 8.04468 12.8365 7.9405 12.9407C7.83631 13.0448 7.77778 13.1862 7.77778 13.3335C7.77778 13.4808 7.83631 13.6221 7.9405 13.7263C8.04468 13.8305 8.18599 13.8891 8.33333 13.8891H11.6667C11.814 13.8891 11.9553 13.8305 12.0595 13.7263C12.1637 13.6221 12.2222 13.4808 12.2222 13.3335C12.2222 13.1862 12.1637 13.0448 12.0595 12.9407C11.9553 12.8365 11.814 12.7779 11.6667 12.7779Z" fill="black"/><path d="M10 3.3335C8.68146 3.3335 7.39252 3.72449 6.2962 4.45703C5.19987 5.18957 4.34539 6.23077 3.8408 7.44894C3.33622 8.66711 3.20419 10.0076 3.46143 11.3008C3.71866 12.594 4.3536 13.7819 5.28595 14.7142C6.2183 15.6466 7.40619 16.2815 8.6994 16.5387C9.9926 16.796 11.333 16.6639 12.5512 16.1594C13.7694 15.6548 14.8106 14.8003 15.5431 13.704C16.2757 12.6076 16.6667 11.3187 16.6667 10.0002C16.6667 8.23205 15.9643 6.53636 14.714 5.28612C13.4638 4.03588 11.7681 3.3335 10 3.3335ZM10 15.5557C8.90121 15.5557 7.8271 15.2299 6.9135 14.6194C5.99989 14.009 5.28782 13.1413 4.86733 12.1262C4.44685 11.111 4.33683 9.994 4.55119 8.91633C4.76555 7.83866 5.29467 6.84875 6.07163 6.07179C6.84859 5.29483 7.83849 4.76572 8.91616 4.55136C9.99384 4.33699 11.1109 4.44701 12.126 4.8675C13.1412 5.28799 14.0088 6.00006 14.6193 6.91366C15.2297 7.82727 15.5556 8.90138 15.5556 10.0002C15.5556 10.7297 15.4119 11.4522 15.1327 12.1262C14.8535 12.8002 14.4443 13.4127 13.9284 13.9285C13.4125 14.4444 12.8 14.8536 12.126 15.1328C11.452 15.412 10.7296 15.5557 10 15.5557Z" fill="black"/></g><defs><clipPath id="clip0_9002_5432"><rect width="20" height="20" fill="white"/></clipPath></defs></svg>
                        </span>
                      </div>
                    </div>
                  {%- endif -%}
                {%- endif -%}
              {%- when 'meta_stitch_product_list' -%} 
                {%- if product.metafields.custom.product_color_stitch_list != blank or product.metafields.custom.product_color_detail_text != blank -%}
                  <div class="Meta_stitch_product_list_main_new" {{ block.shopify_attributes }}>
                    <div class="Meta_stitch_product_list_inner_new">

                      {%- if product.metafields.custom.product_color_detail_text != blank -%}
                        <div class="Meta_stitch_product_list_maintitle_new">
                          <div class="Meta_stitch_product_list_innertitle_new">
                            {{ block.settings.main_heading_texts }}<span>{{ product.metafields.custom.product_color_detail_text }}</span>
                          </div>
                        </div>
                      {%- endif -%}

                      {%- if product.metafields.custom.product_color_stitch_list != blank -%}
                        <div class="Meta_stitch_product_list_mainloops_new">
                          <div class="Meta_stitch_product_list_innerloops_new">
                            {% assign stitcherProducts = product.metafields.custom.product_color_stitch_list.value.choose_products %}
                            {% for product_color_stitch_list in stitcherProducts.value %}
                              {%- if product_color_stitch_list.metafields.custom.product_color_image.value != blank -%}
                                {%- if product_color_stitch_list.metafields.custom.product_color_image.value contains "files" -%}
                                  <div  class="Meta_stitch_product_list_innerloops_mainboxs_new">
                                    <a href="{{ product_color_stitch_list.url }}" data-product-handle="{{ product_color_stitch_list.handle }}" class="{% if product.id == product_color_stitch_list.id %} active{% endif %}">
                                      <div class="Meta_stitch_product_list_innerloops_innerboxs_new">
                                        {{
                                          product_color_stitch_list.metafields.custom.product_color_image.value | 
                                          image_url: width: img.width | 
                                          image_tag: widths: '750, 900, 1080, 1296, 1512, 1728, 1944, 2048', 
                                          fetchpriority: 'auto', 
                                          class:'Static_image_new'
                                        }}
                                      </div>
                                    </a>
                                  </div>
                                {%- endif -%}  
                              {%- endif -%}  
                            {%- endfor -%}
                          </div>
                        </div>
                      {%- endif -%}

                    </div>
                  </div>
                {%- endif -%}  

