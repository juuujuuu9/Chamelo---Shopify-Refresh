{%- assign comparison_table_item_selections = shop.metaobjects.comparison_table_item_selections -%}
{%- paginate shop.metaobjects.comparison_table_item_selections.values by 1000 -%}
  {%- for comparison_table_item_selections in comparison_table_item_selections.values -%}
    <div class="Index_object_table_detail_main_new" style="background: {{ section.settings.main_background_color }};">
      <div class="page-width">
        <div class="Index_object_table_detail_inner_new">
          {%- if comparison_table_item_selections.main_detail_text.value != blank or comparison_table_item_selections.main_heading_text.value != blank -%}
            <div class="Index_object_table_detail_mainboths_new">
              <div class="Index_object_table_detail_innerboths_new">
                {%- if comparison_table_item_selections.main_detail_text.value != blank -%}
                  <div class="Index_object_table_detail_innerboths_maintexts_new">
                    <div class="Index_object_table_detail_innerboths_innertexts_new">
                      <h5>{{ comparison_table_item_selections.main_detail_text.value }}</h5>
                    </div>
                  </div>
                {%- endif -%}
                {%- if comparison_table_item_selections.main_heading_text.value != blank -%}
                  <div class="Index_object_table_detail_innerboths_maintitle_new">
                    <div class="Common_title_new Index_object_table_detail_innerboths_innertitle_new">
                      <h2>{{ comparison_table_item_selections.main_heading_text.value }}</h2>
                    </div>
                  </div>
                {%- endif -%}
              </div>
            </div>
          {%- endif -%}
        
          {%- if comparison_table_item_selections.main_column_list_1.value != blank or comparison_table_item_selections.main_column_list_2.value != blank or comparison_table_item_selections.main_column_list_3.value != blank or comparison_table_item_selections.main_column_list_4.value != blank or comparison_table_item_selections.main_column_list_5.value != blank -%}
            <div class="Index_object_table_detail_maintable_new Tabs_common_main_new">
              <div class="Index_object_table_detail_innertable_new">
                {%- if comparison_table_item_selections.main_column_list_3.value != blank or comparison_table_item_selections.main_column_list_4.value != blank or comparison_table_item_selections.main_column_list_5.value != blank -%}
                  <div class="Index_object_table_detail_innertable_maindropdown_new Mobile_only_new">
                    <div class="Index_object_table_detail_innertable_innerdropdown_new">
                      <div class="tab_title_main_new">
                        <div class="tab_title_mobile_new">
                          {%- if comparison_table_item_selections.main_column_list_3.value != blank -%}
                            <div class="tab_title_mobile_inner_new">
                              {%- for main_column_list_3 in comparison_table_item_selections.main_column_list_3.value.main_column_list.value limit: 1 -%}
                                {%- if main_column_list_3 != blank -%}
                                  vs. {{ main_column_list_3 }}
                                {%- endif -%}
                              {%- endfor -%}
                            </div>
                          {%- endif -%}
                        </div>
                        <div class="tab common_tab_new">
                          {%- if comparison_table_item_selections.main_column_list_3.value != blank -%}
                            <button class="tablinks Tabs_common_inner_new active slide-1" slide_id="slide_1">
                              {%- for main_column_list_3 in comparison_table_item_selections.main_column_list_3.value.main_column_list.value limit: 1 -%}
                                {%- if main_column_list_3 != blank -%}
                                  vs. {{ main_column_list_3 }}
                                {%- endif -%}
                              {%- endfor -%}
                            </button>
                          {%- endif -%}
                          {%- if comparison_table_item_selections.main_column_list_4.value != blank -%}
                            <button class="tablinks Tabs_common_inner_new slide-2" slide_id="slide_2">
                              {%- for main_column_list_4 in comparison_table_item_selections.main_column_list_4.value.main_column_list.value limit: 1 -%}
                                {%- if main_column_list_4 != blank -%}
                                  vs. {{ main_column_list_4 }}
                                {%- endif -%}
                              {%- endfor -%}
                            </button>
                          {%- endif -%}
                          {%- if comparison_table_item_selections.main_column_list_5.value != blank -%}
                            <button class="tablinks Tabs_common_inner_new slide-3" slide_id="slide_3">
                              {%- for main_column_list_5 in comparison_table_item_selections.main_column_list_5.value.main_column_list.value limit: 1 -%}
                                {%- if main_column_list_5 != blank -%}
                                  vs. {{ main_column_list_5 }}
                                {%- endif -%}
                              {%- endfor -%}
                            </button>
                          {%- endif -%}
                        </div>
                      </div>  
                    </div>
                  </div>
                {%- endif -%}
                <div class="Index_object_table_detail_innertable_mainsides_new">
                  <div class="Index_object_table_detail_innertable_innersides_new">
                    {%- if comparison_table_item_selections.main_column_list_1.value != blank -%}
                      <div class="Index_object_table_detail_innertable_innersides_mainleft_new">
                        <div class="Index_object_table_detail_innertable_innersides_innerleft_new">
                          <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                            <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
    
                            </div>
                          </div>
                          {%- for main_column_list_1 in comparison_table_item_selections.main_column_list_1.value.main_column_list.value -%}
                            {%- if main_column_list_1 != blank -%}
                              <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                                <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
                                  {{ main_column_list_1 }}
                                </div>
                              </div>
                            {%- endif -%}
                          {%- endfor -%}  
                        </div>
                      </div>
                    {%- endif -%}
                    {%- if comparison_table_item_selections.main_column_list_2.value != blank or comparison_table_item_selections.main_column_list_3.value != blank or comparison_table_item_selections.main_column_list_4.value != blank or comparison_table_item_selections.main_column_list_5.value != blank -%}
                      <div class="Index_object_table_detail_innertable_innersides_mainright_new">
                        <div class="Index_object_table_detail_innertable_innersides_innerright_new">
                          {%- if comparison_table_item_selections.main_column_list_2.value != blank -%}
                            <div class="Index_object_table_detail_innertable_innersides_innerright_1_new">
                              {%- if comparison_table_item_selections.main_video_or_image_background_column_list_2.value != blank -%}
                                <div class="Index_object_table_detail_innertable_innersides_innerright_1_mainstatic_new">
                                  <div class="Index_object_table_detail_innertable_innersides_innerright_1_innerstatic_new">
                                    {%- if comparison_table_item_selections.main_video_or_image_background_column_list_2.value contains "files" -%}
                                      <div class="Index_object_table_detail_innertable_innersides_innerright_1_innerstatic_imagevideo_new">
                                        {{
                                          comparison_table_item_selections.main_video_or_image_background_column_list_2.value | 
                                          image_url: width: img.width | 
                                          image_tag: widths: '750, 900, 1080, 1296, 1512, 1728, 1944, 2048', 
                                          fetchpriority: 'auto', 
                                          class:'Static_image_new'
                                        }}
                                      </div>
                                    {%- else -%}
                                      <div class="Index_object_table_detail_innertable_innersides_innerright_1_innerstatic_imagevideo_new">
                                        {{ comparison_table_item_selections.main_video_or_image_background_column_list_2.value | video_tag: class:'Static_video_new', image_size: 'master', controls: false, autoplay: true, loop: true }}
                                      </div>
                                    {%- endif -%}
                                  </div>
                                </div>
                              {%- endif -%}
                              <div class="Index_object_table_detail_innertable_innersides_innerright_1_leftside_new">
                                {%- if comparison_table_item_selections.main_logo_image_column_list_2.value != blank -%}
                                  <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                                    <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
                                      {%- if comparison_table_item_selections.main_logo_image_column_list_2.value contains "files" -%}
                                        <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_logoimage_new">
                                          {{
                                            comparison_table_item_selections.main_logo_image_column_list_2.value | 
                                            image_url: width: img.width | 
                                            image_tag: widths: '750, 900, 1080, 1296, 1512, 1728, 1944, 2048', 
                                            fetchpriority: 'auto', 
                                            class:'Static_image_new'
                                          }}
                                        </div>
                                      {%- endif -%}
                                    </div>
                                  </div>
                                {%- endif -%}
                                {%- for main_column_list_2 in comparison_table_item_selections.main_column_list_2.value.main_column_list.value -%}
                                  {%- if main_column_list_2 != blank -%}
                                    <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                                      <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
                                        {{ main_column_list_2 }}
                                      </div>
                                    </div>
                                  {%- endif -%}
                                {%- endfor -%} 
                              </div>
                            </div>
                          {%- endif -%}
                          {%- if comparison_table_item_selections.main_column_list_3.value != blank or comparison_table_item_selections.main_column_list_4.value != blank or comparison_table_item_selections.main_column_list_5.value != blank -%}
                            <div class="Index_object_table_detail_innertable_innersides_innerright_2_new">
                              <div class="Index_object_table_detail_innertable_innersides_innerright_2_rightside_new">
                                <div class="swiper Index_object_table_slider">
                                  <div class="swiper-wrapper">
                                    {%- if comparison_table_item_selections.main_column_list_3.value != blank -%}
                                      <div class="swiper-slide Index_object_table_detail_innertable_innersides_innerright_2_rightside_mainloop_new">
                                        <div class="Index_object_table_detail_innertable_innersides_innerright_2_rightside_innerloop_new">
                                          {%- for main_column_list_3 in comparison_table_item_selections.main_column_list_3.value.main_column_list.value -%}
                                            {%- if main_column_list_3 != blank -%}
                                              <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                                                <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
                                                  {{ main_column_list_3 }}
                                                </div>
                                              </div>
                                            {%- endif -%}
                                          {%- endfor -%} 
                                        </div>
                                      </div>
                                    {%- endif -%}
                                    {%- if comparison_table_item_selections.main_column_list_4.value != blank -%}
                                      <div class="swiper-slide Index_object_table_detail_innertable_innersides_innerright_2_rightside_mainloop_new">
                                        <div class="Index_object_table_detail_innertable_innersides_innerright_2_rightside_innerloop_new">
                                          {%- for main_column_list_4 in comparison_table_item_selections.main_column_list_4.value.main_column_list.value -%}
                                            {%- if main_column_list_4 != blank -%}
                                              <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                                                <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
                                                  {{ main_column_list_4 }}
                                                </div>
                                              </div>
                                            {%- endif -%}
                                          {%- endfor -%} 
                                        </div>
                                      </div>
                                    {%- endif -%}
                                    {%- if comparison_table_item_selections.main_column_list_5.value != blank -%}
                                      <div class="swiper-slide Index_object_table_detail_innertable_innersides_innerright_2_rightside_mainloop_new">
                                        <div class="Index_object_table_detail_innertable_innersides_innerright_2_rightside_innerloop_new">
                                          {%- for main_column_list_5 in comparison_table_item_selections.main_column_list_5.value.main_column_list.value -%}
                                            {%- if main_column_list_5 != blank -%}
                                              <div class="Index_object_table_detail_innertable_innersides_innerleft_mainboxs_new">
                                                <div class="Index_object_table_detail_innertable_innersides_innerleft_innerboxs_new">
                                                  {{ main_column_list_5 }}
                                                </div>
                                              </div>
                                            {%- endif -%}
                                          {%- endfor -%} 
                                        </div>
                                      </div>
                                    {%- endif -%}
                                  </div>
                                </div>
                              </div>
                            </div>
                          {%- endif -%}
                        </div>
                      </div>
                    {%- endif -%}
                  </div>
                  <div class="swiper-pagination-table"></div>
                </div>
              </div>
            </div>
          {%- endif -%}
        </div>
      </div>
    </div>
  {%- endfor -%}
{%- endpaginate -%}

{% schema %}
  {
    "name": "Index Object Table Detail",
    "class": "Index_object_table_detail_new",
    "settings": [
      {
        "type": "color",
        "id": "main_background_color",
        "label": "Add Main Background Color",
        "default": "#FFFFFF"
      }
    ],
    "presets": [
      {
        "name": "Index Object Table Detail"
      }
    ]
  }
{% endschema %}