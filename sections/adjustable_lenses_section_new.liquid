                    {% for block in section.blocks %}
                        {% if block.type == "tab" %}
                            <div 
                            {% if block.settings.lottie_url != "blank" %}
                                lottie-url = "{{ block.settings.lottie_url }}"
                            {% endif %} 
                            class="hb-tab-section__tab_img-wrap {% if forloop.first %}active{% endif %}" js-tab-image_content>
