{{ "hb-tab-section.css" |  asset_url |  stylesheet_tag }}

<style>
    @media screen and (max-width:767px){
        #hb_tab_section-{{ section.id }} {
        padding-top: {{ section.settings.padding_top_mob | times:0.75 }}px;
        padding-bottom: {{ section.settings.padding_bottom_mob | times:0.75 }}px;
      }  
    }
  #hb_tab_section-{{ section.id }} {
        padding-top: {{ section.settings.padding_top_mob }}px;
        padding-bottom: {{ section.settings.padding_bottom_mob }}px;
      } 
  .adjustable_lenses_sec  h2.hb_tab_section__title{text-transform:none;}
  .adjustable_lenses_sec .hb-tab-section__tab-title,.adjustable_lenses_sec .hb-tab-section__sub_title{margin: 0 0 16px;}
  .adjustable_lenses_sec  .hb-tab-section__flex{display:none;}
  .adjustable_lenses_sec .hb_tab_section__tab-content{background:transparent;}
  .adjustable_lenses_sec .hb_tab_section__tab-alpha{padding:14px;}
  .adjustable_lenses_sec .tab-contents .hb_tab_section__text p b{font-weight:600;}
  .adjustable_lenses_sec .hb_tab_section__inner-row{padding:24px 0 0;}
  .adjustable_lenses_sec .hb_tab_section__row{background:#F6F6F6;padding:12px;gap:16px;}
  .adjustable_lenses_sec .hb_tab_section__btn{margin-top:32px;}
  .adjustable_lenses_sec .hb_tab_section__center-content .hb_tab_section__text p{font-size: 14px;}
  .adjustable_lenses_sec .hb-tab-section__tab-inner-content .hb_tab_section__text p br{content:' ';}
  .adjustable_lenses_sec .hb-tab-section__tab-inner-content .hb_tab_section__text p br:before{content:' ';}
  .adjustable_lenses_sec .hb_tab_section__text h4{display: flex;align-items: center;margin: 0 0 8px;text-transform:uppercase;color:#000;font-size: 14px;font-weight: 600;line-height: 17.5px;text-align: left;}
  .adjustable_lenses_sec .hb_tab_section__text h4 em{margin-left: 12px;color:#000;font-style:normal;font-size: 12px;font-weight: 400;line-height: 12px;text-align: left;text-decoration-line: underline;text-underline-offset: 3px;text-decoration-thickness:0.79px;}
  .adjustable_lenses_sec .hb_tab_section__text .popup_text_visible{margin-left: 0px;text-decoration:none;}
  .adjustable_lenses_sec .hb_tab_section__text .popup_text_visible svg{width:12px;height:12px;display: inline;margin-left: 4px;}
  .adjustable_lenses_sec .button_bottom_text svg{width:auto;height:12px !important;margin-right: 8px;}
  .adjustable_lenses_sec .button_bottom_text{font-family:var(--font-mono-regular-family);display:flex;color:#000;justify-content:center;align-items:center;margin-top:12px;font-size: 12px;font-weight: 400;line-height: 16.2px;text-align: center;}
  .adjustable_lenses_sec .hb_product_slider{display:none!important;}
  @media(min-width:1200px){
    .adjustable_lenses_sec .hb_tab_section__tab-alpha{margin-right: 32px;}
  }
  @media(max-width:749px){
    .hb_tab_section.adjustable_lenses_sec{padding:48px 0!important;}
    .adjustable_lenses_sec .hb_tab_section__tab-alpha .hb-tab-section__tab-inner-content{display:block;}
    .adjustable_lenses_sec .hb_tab_section__center-content{display:none;}
    .adjustable_lenses_sec .hb_tab_section__tab-alpha {padding: 0 0 0px;}
    .adjustable_lenses_sec .hb_tab_section__tab-beta{margin-top: 24px;}
    .adjustable_lenses_sec .hb_tab_section__top-video video{aspect-ratio: 1 / 0.64;}
    .adjustable_lenses_sec .hb-tab-section__tab-inner-content .hb_tab_section__text p br:before{display:none;}
    .adjustable_lenses_sec .hb-tab-section__tab-inner-content .hb_tab_section__text p br {width: 100%;display: block;content: '';height: 12px;}
    .adjustable_lenses_sec .hb_tab_section__top-video {margin-top:20px;margin-bottom: 20px;margin-left: -20px;margin-right: -20px;}
    .adjustable_lenses_sec .hb-tab-section__tab-inner-content .hb_tab_section__text p{max-width: 100%;}
    .adjustable_lenses_sec a.hb_tab_section__btn.large-up-hide.medium-up-hid {display: block;margin-top: 24px;padding: 14px 30px;font-size: 16px;font-weight: 700;line-height: 20px;letter-spacing: 0.01em;}
    .adjustable_lenses_sec .hb-tab-section__sub_title.large-up-hide.medium-hide {margin: 0 0 8px;}
    .adjustable_lenses_sec .hb-tab-section__tab-inner-content .hb_tab_section__text.large-up-hide p {max-width: 334px;}
    .adjustable_lenses_sec .hb-tab-section__sub_title h2.hb_tab_section__title{font-size: 40px;font-weight: 700;line-height: 40px;letter-spacing: -0.01em;}
    .adjustable_lenses_sec .hb-tab-section__tab_img-wrap{height:220px;}

  }
  
</style>

<script src="{{ 'lottie.js' |  asset_url }}" type="module"></script>

<div class="hb_tab_section adjustable_lenses_sec" id="hb_tab_section-{{ section.id }}">

    {% if section.settings.title != blank %}
        <h2 class="hb_tab_section__title mobile">{{ section.settings.title }}</h2>
    {% endif %}
    {% if section.settings.Top_img != blank %}
        <div class="hb_tab_section__top-img">
            <img src="{{ section.settings.Top_img |  image_url: width:  "2000x"  }}" alt="">
        </div>
    {% endif %}
   
  
    <div class="page-width">
        <div class="hb_tab_section__tab-content">
            <div class="hb_tab_section__tab-alpha">
                {% if section.settings.tab_title != blank %}
                    <h4 class="hb-tab-section__tab-title">{{ section.settings.tab_title }}</h4>
                {% endif %}
                <div class="hb-tab-section__flex">
                    {% for block in section.blocks %}
                        {% if block.type == "tab" %}
                            <h4 class="hb-tab-section__tab_text {% if forloop.first %}active{% endif %}">{{ block.settings.tab_text }}</h4>
                        {% endif %}
                    {% endfor %}
                </div>
                <div class="tab-contents">
                    {% for block in section.blocks %}
                        {% if block.type == "tab" %}
                            <div class="hb-tab-section__tab-inner-content  {% if forloop.first %}active{% endif %}" js-desktop-tab_content>
                                {% if block.settings.tab_sub_title != blank %}
                                    <div class="hb-tab-section__sub_title"><h2 class="hb_tab_section__title">{{ block.settings.tab_sub_title }}</h2></div>
                                {% endif %}
                               {% if section.settings.video_mobile != blank %}
        <div class="hb_tab_section__top-video large-up-hide medium-hide">
            {{ section.settings.video_mobile | video_tag: autoplay: true, muted: true, loop: true, controls: false }}
        </div>
    {% endif %}
                                {% if block.settings.tab_sub_text != blank %}
                                    <div class="hb_tab_section__text">{{ block.settings.tab_sub_text }}</div>
                                {% endif %}
                                {% if block.settings.popup_title != blank or block.settings.popup_text != blank %}
                                    <div class="hb_tab_section__popup-container">
                                        {% if block.settings.popup_title != blank %}
                                            <h4 class="hb_tab_section__popup-title">{{ block.settings.popup_title }}</h4>
                                        {% endif %}
                                     
                                        {% if block.settings.popup_text != blank %}
                                            <p class="hb_tab_section__popup-text">{{ block.settings.popup_text }}</p>
                                        {% endif %}
                                    </div>
                                {% endif %}
                                {% if section.settings.title != blank or section.settings.video != blank or section.settings.text != blank %}
        <div class="hb_tab_section__center-content" id="{{ section.settings.id }}">
            <div class="">
                <div class="hb_tab_section__inner-row">
                    {% if section.settings.title != blank %}
                        <h2 class="hb_tab_section__title desktop">{{ section.settings.title }}</h2>
                    {% endif %}
                    <div class="hb_tab_section__row" style="max-width:{{ section.settings.width }}px">
                        {%- if section.settings.video != blank -%}
                        <div class="hb_tab_section__video" style="max-width:{{ section.settings.video_width }}px;flex:{{ section.settings.video_width }}px;height:{{ section.settings.video_height }}px;">
                            {{ section.settings.video | video_tag: autoplay: true, muted: true, loop: true, controls: false }}
                        </div>
                        {%- endif -%}
                        {% if section.settings.greybox_title != blank or section.settings.greybox_subtitle != blank or section.settings.greybox_description != blank %}
                            <div class="hb_tab_section__text">
                              <h4>{{ section.settings.greybox_title }} <span class="popup_text_visible"><em>{{ section.settings.greybox_subtitle }}</em><svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="10" height="10" viewBox="0 0 24 24">
    <path d="M 12 2 C 6.4889971 2 2 6.4889971 2 12 C 2 17.511003 6.4889971 22 12 22 C 17.511003 22 22 17.511003 22 12 C 22 6.4889971 17.511003 2 12 2 z M 12 4 C 16.430123 4 20 7.5698774 20 12 C 20 16.430123 16.430123 20 12 20 C 7.5698774 20 4 16.430123 4 12 C 4 7.5698774 7.5698774 4 12 4 z M 11 7 L 11 9 L 13 9 L 13 7 L 11 7 z M 11 11 L 11 17 L 13 17 L 13 11 L 11 11 z"></path>
</svg></span></h4><p>{{ section.settings.greybox_description }}</p>
                              
                            </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    {% endif %}
                            </div>
                        {% endif %}
                    {% endfor %}
                </div>
                {% if section.settings.btn_text != blank %}
                    <a href="{{ section.settings.btn_url }}" class="hb_tab_section__btn">{{ section.settings.btn_text }}</a>
